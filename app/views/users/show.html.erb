<html>
  <head>
    <%= stylesheet_link_tag 'users_styles' %>
  </head>
 
  <body>
  

  <div class="container-user">
  <img src="" class="img-user-s">
  <ul class="ul-user">
    <li>User Name: <%= @user.name %></li>
    <li>User Posts: <%= @user.post_counter %></li>
  </ul>
</div>

<div class="user-bio">
    <h4 class="bio-p"> Bio of <%= @user.name %></h4>
    <p><%= @user.bio %></p>
</div>

<div class="users-posts">
    <%= render_user_posts(@user) %>
</div>

<button class="btn btn-primary">see all posts</button>


</body>
</html>


  







